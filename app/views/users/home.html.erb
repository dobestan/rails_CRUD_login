<h1>USER LIST</h1>
<table>
    <tr>
        <th>USERNAME</th>
        <th>PASSWORD(MD5)</th>
    </tr>
    <% @users.each do |user| %>
        <tr>
            <td><%= user.username  %></td>
            <td><%= user.password  %></td>
            <td><a href="/users/edit/<%= user.id %>">change password</a></td>
            <td><a href="/users/destroy/<%= user.id %>">delete user</a></td>
        </tr>
    <% end %>
</table>
